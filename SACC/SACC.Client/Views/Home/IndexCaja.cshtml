@{
    Layout = "~/Views/Shared/_LayoutCaja.cshtml";
}


@section Scripts {
    @if (TempData["lblNombre"] != null)
    {
        <script type="text/javascript">
            $("#lblNombre").html('@TempData["lblNombre"]');
            $("#imgFoto").attr('src', '@TempData["imgFoto"]');
        </script>
    };
}


<div class="content animate-panel">
    <div class="row">
        <div class="col-lg-12 text-center m-t-md">
            <h2>Bienvenido(a) al @System.Configuration.ConfigurationManager.AppSettings["NombreAplicacionCaja"].ToString()</h2>
        </div>
    </div>
</div>