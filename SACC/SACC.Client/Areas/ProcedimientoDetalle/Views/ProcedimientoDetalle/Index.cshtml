@{
    Layout = "~/Views/Shared/_LayoutTarifa.cshtml";
}


@section Scripts {
    @if (TempData["lblNombre"] != null)
    {
        <script type="text/javascript">
            $("#lblNombre").html('@TempData["lblNombre"]');
            $("#imgFoto").attr('src', '@TempData["imgFoto"]');
        </script>
    };

    <script type="text/javascript" src="@System.Configuration.ConfigurationManager.AppSettings["UrlPrincipal"].ToString()Scripts/Areas/ProcedimientoDetalle/ProcedimientoDetalle.js"></script>
}


<div class="small-header transition animated fadeIn">
    <div class="hpanel">
        <div class="panel-body">
            <div id="hbreadcrumb" class="pull-right">
                <ol class="hbreadcrumb breadcrumb">
                    <li><span>Procedimientos </span></li>
                    <li><span>Procedimeinto Detalle </span></li>
                    <li class="active"><span>Estructura </span></li>
                </ol>
            </div>
            <h2 class="font-light m-b-xs">Procedimiento Detalle</h2>
            <small>Estructura.</small>
        </div>
    </div>
</div>


<div class="content animate-panel">
    <div class="row rowDatosGenerales">
        <div class="col-lg-12">
            <div class="hpanel">
                <ul class="nav nav-tabs">
                    <li class="active"><a data-toggle="tab" href="#tab-1" aria-expanded="true"> Datos del Procedimiento</a></li>
                </ul>
                <div class="tab-content">
                    <div id="tab-1" class="tab-pane active">
                        <div class="panel-body">
                            <div role="form" class="form-horizontal">
                                <div class="col-sm-12">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">Folio</label>
                                                <div class="col-sm-10">
                                                    <input type="text" id="txtProcedimientoId" name="txtProcedimientoId" class="form-control input-sm" , readonly="readonly" value="@ViewBag.ProcedimientoId" />
                                                    <span class="field-validation-valid text-danger" data-valmsg-for="ProcedimientoId" data-valmsg-replace="true"></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-7">
                                            <div class="form-group">
                                                <label class="col-sm-2 control-label">Procedimiento</label>
                                                <div class="col-sm-10">
                                                    <input type="text" id="txtProcedimientoDescripcion" name="txtContratante" class="form-control input-sm" , readonly="readonly" value="@ViewBag.ProcedimientoDescripcion" />
                                                    <span class="field-validation-valid text-danger" data-valmsg-for="ProcedimientoDescripcion" data-valmsg-replace="true"></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2">
                                            <div class="form-group">
                                                <div class="col-sm-10">
                                                    <button class="btn btn-default btn-sm" onclick="ProcedimientoDetalle.redirecciona('PROCEDIMIENTO_INDEX');">Regresar</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="hpanel">
                <div class="panel-heading">
                    @*<button onclick="ProcedimientoDetalle.AbreModalPersona()" class="btn btn-primary btn-sm">Agregar</button>*@
                </div>
                <table id="dgDatosProcedimientoDetalle" data-height="430" data-classes="table table-hover table-condensed" data-striped="true" data-pagination="true"></table>
            </div>
        </div>
    </div>
</div>


<input type="hidden" id="txtApuntadorProcedimientoDetalle" value="PROCEDIMIENTO_DETALLE_INDEX" placeholder="APUNTADOR" />